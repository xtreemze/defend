{
  "name": "defend",
  "version": "1.0.0",
  "description": "Tower Defense WebGame",
  "sideEffects": false,
  "scripts": {
    "archive": "node ./node/archive.js",
    "manifest": "node ./node/generateManifest.js",
    "commits": "git log --graph --oneline --decorate -5",
    "queryVersion": "node ./node/queryVersion.js",
    "pwa": "npx pwabuilder https://xtreemze.github.io/defend/ -d ./release/pwa -f w3c -i ./release/screenshot3.png",
    "pwaWeb": "npx pwabuilder -m ./dist/site.webmanifest -d ./dist -f w3c -i ./release/screenshot3.png -p web",
    "lint": "npx eslint ./src/js --quiet",
    "clean": "node ./node/clean.js && npm cache clean && npm i -D -f && npm up -D --depth 2 && npm up --save && npm outdated --depth 2",
    "update": "npm up -D --depth 2 && npm outdated --depth 2",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "dev": "webpack-dev-server --config webpack.configDev.js --mode development --hot --host 0.0.0.0",
    "soundTest": "webpack-dev-server --config webpack.configSoundTest.js --mode development --hot --host 0.0.0.0",
    "preexportp": "npm run queryVersion && node ./node/rmDir.js && node ./node/cpDir.js",
    "exportp": "webpack --mode production && npm run favicon && npm run manifest",
    "posttoTest": "npm config set sign-git-tag true && git add -A && npm version patch -s --force && npm run commits & npm run queryVersion && git push && git push --tags",
    "favicon": "real-favicon generate faviconDescription.json faviconData.json ./dist/ && real-favicon inject faviconData.json ./dist/ ./dist/index.html",
    "faviconUpdate": "real-favicon check-for-update --fail-on-update faviconData.json",
    "screenshot": "node ./node/puppeteer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtreemze/defend.git"
  },
  "author": "Carlos Velasco",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/xtreemze/defend/issues"
  },
  "homepage": "https://github.com/xtreemze/defend#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {},
  "devDependencies": {
    "airbnb-browser-shims": "^3.0.0",
    "autoprefixer": "^9.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-env": "^1.7.0",
    "babylonjs": "^3.3.0-alpha.15",
    "babylonjs-inspector": "^3.3.0-alpha.15",
    "browserslist": "^4.0.1",
    "cannon": "^0.6.2",
    "cli-real-favicon": "0.0.6",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "fs-extra": "^7.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.14.0",
    "puppeteer": "^1.6.2",
    "style-loader": "^0.21.0",
    "tone": "^0.12.80",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "workbox-webpack-plugin": "^3.4.1"
  },
  "main": "index.html",
  "keywords": [
    "BabylonJS",
    "PWA",
    "JavaScript",
    "Mobile",
    "Game"
  ]
}
