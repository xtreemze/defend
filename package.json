{
    "name": "defend",
    "version": "1.0.2",
    "description": "In “Defend” you’re placed in the center of a stronghold contained within a 3D world. You will be immediately faced with a constant onslaught of attacks from aggressive spherical objects who seek to destroy your energy reserves as this is the only source of life in this world. Repel the attacks and maintain your stronghold with a sustainable level of resources.",
    "sideEffects": false,
    "scripts": {
        "archive": "node ./node/archive.js",
        "manifest": "node ./node/generateManifest.js",
        "queryVersion": "node ./node/queryVersion.js",
        "pwa": "npx pwabuilder https://xtreemze.github.io/defend/ -d ./dist -f w3c -i ./release/screenshot3.png -p web",
        "pwaWeb": "npx pwabuilder -m ./dist/site.webmanifest -d ./dist -f w3c -i ./release/screenshot3.png -p web",
        "lint": "eslint ./src/js/** --quiet --fix",
        "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
        "dev": "webpack-dev-server --config webpack.configDev.js --mode development --hot",
        "soundTest": "webpack-dev-server --config webpack.configSoundTest.js --mode development --hots",
        "preexportp": "npm run queryVersion && node ./node/rmDir.js && node ./node/cpDir.js",
        "exportp": "webpack --mode production && npm run favicon && npm run manifest",
        "build": "webpack --mode production --watch",
        "posttoTest": "npm config set sign-git-tag true && git add -A && npm version patch -s --force && npm run commits & npm run queryVersion && git push && git push --tags",
        "favicon": "npm run pwa && real-favicon generate faviconDescription.json faviconData.json ./dist/ && real-favicon inject faviconData.json ./dist/ ./dist/index.html",
        "faviconUpdate": "real-favicon check-for-update --fail-on-update faviconData.json",
        "screenshot": "node ./node/puppeteer.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xtreemze/defend.git"
    },
    "author": "Carlos Velasco",
    "private": true,
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/xtreemze/defend/issues"
    },
    "homepage": "https://github.com/xtreemze/defend#readme",
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "dependencies": {
        "eslint-plugin-tslint": "^3.1.0",
        "typescript-eslint-parser": "^22.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.0",
        "airbnb-browser-shims": "^3.1.0",
        "autoprefixer": "^9.4.3",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.4",
        "babel-preset-airbnb": "^3.2.0",
        "babel-preset-env": "^1.7.0",
        "babylonjs": "^3.3.0",
        "babylonjs-inspector": "^3.3.0",
        "browserslist": "^4.3.6",
        "cannon": "^0.6.2",
        "cli-real-favicon": "0.0.7",
        "css-loader": "^2.0.2",
        "eslint": "^5.10.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-prettier": "^3.0.0",
        "extract-loader": "^3.1.0",
        "file-loader": "^3.0.1",
        "fs-extra": "^7.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "prettier": "^1.15.3",
        "puppeteer": "^1.11.0",
        "style-loader": "^0.23.1",
        "tone": "^13.4.9",
        "ts-loader": "^5.3.1",
        "ts-node": "^8.1.0",
        "tslint": "^5.12.0",
        "typescript": "^3.2.2",
        "url-loader": "^1.1.2",
        "webpack": "^4.28.0",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10",
        "workbox-webpack-plugin": "^4.3.0"
    },
    "main": "index.html",
    "keywords": [
        "BabylonJS",
        "PWA",
        "JavaScript",
        "Mobile",
        "Game"
    ]
}
