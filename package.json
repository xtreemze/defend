{
  "name": "defend",
  "version": "0.0.1",
  "description": "Tower Defense WebGame",
  "sideEffects": false,
  "scripts": {
    "archive": "node ./node/archive.js",
    "manifest": "node ./node/generateManifest.js",
    "commits": "git log --graph --oneline --decorate -5",
    "queryVersion": "node ./node/queryVersion.js",
    "lint": "npx eslint ./src/js --quiet",
    "clean": "node ./node/clean.js && npm cache clean && npm i -D -f && npm up -D --depth 2 && npm up --save && npm outdated --depth 2",
    "update": "npm up -D --depth 2 && npm outdated --depth 2",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "dev": "webpack-dev-server --mode development --hot --host 0.0.0.0",
    "preexportp": "npm run queryVersion && node ./node/rmDir.js && node ./node/cpDir.js",
    "exportp": "webpack --mode production && npm run favicon",
    "posttoTest": "npm config set sign-git-tag true && git add -A && npm version patch -s --force && npm run commits & npm run queryVersion && git push && git push --tags",
    "favicon": "real-favicon generate faviconDescription.json faviconData.json ./dist/ && real-favicon inject faviconData.json ./dist/ ./dist/index.html",
    "faviconUpdate": "real-favicon check-for-update --fail-on-update faviconData.json",
    "screenshot": "node ./node/puppeteer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtreemze/defend.git"
  },
  "author": "Carlos Velasco",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/xtreemze/defend/issues"
  },
  "homepage": "https://github.com/xtreemze/defend#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {},
  "devDependencies": {
    "airbnb-browser-shims": "^3.0.0",
    "archiver": "^2.1.1",
    "autoprefixer": "^8.6.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-airbnb": "^2.5.1",
    "babel-preset-env": "^1.7.0",
    "babylonjs": "^3.3.0-alpha.11",
    "babylonjs-inspector": "^3.3.0-alpha.11",
    "babylonjs-materials": "^3.3.0-alpha.11",
    "browserslist": "^4.0.0",
    "cannon": "^0.6.2",
    "cli-real-favicon": "0.0.6",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.0",
    "cssnano-preset-advanced": "^4.0.0",
    "earcut": "^2.1.3",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "html-loader": "^0.5.5",
    "html-minifier-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "offline-plugin": "^5.0.5",
    "oimo": "^1.0.9",
    "postcss": "^6.0.23",
    "postcss-loader": "^2.1.6",
    "postcss-scss": "^1.0.6",
    "postcss-uncss": "^0.16.1",
    "precss": "^3.1.2",
    "prettier": "^1.13.7",
    "puppeteer": "^1.5.0",
    "request": "^2.87.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "tone": "^0.12.80",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "uncss": "^0.16.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "main": "index.html",
  "keywords": [
    "BabylonJS",
    "PWA",
    "JavaScript",
    "Mobile",
    "Game"
  ]
}
