{
  "name": "defend",
  "version": "0.0.1",
  "description": "Tower Defense WebGame",
  "sideEffects": false,
  "scripts": {
    "archive": "node ./node/archive.js",
    "manifest": "node ./node/generateManifest.js",
    "commits": "git log --graph --oneline --decorate -5",
    "queryVersion": "node ./node/queryVersion.js",
    "lint": "npx eslint ./src/js --quiet",
    "clean": "node ./node/clean.js && npm cache clean && npm i -D -f && npm up -D --depth 2 && npm up --save && npm outdated --depth 2",
    "update": "npm up -D --depth 2 && npm outdated --depth 2",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "dev": "webpack-dev-server --mode development --hot",
    "preexportp": "npm run queryVersion && node ./node/rmDir.js && npm run manifest && node ./node/cpDir.js",
    "exportp": "webpack --mode production",
    "posttoTest": "npm config set sign-git-tag true && git add -A && npm version patch -s --force && npm run commits & npm run queryVersion && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtreemze/defend.git"
  },
  "author": "Carlos Velasco",
  "private": true,
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/xtreemze/defend/issues"
  },
  "homepage": "https://github.com/xtreemze/defend#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {},
  "devDependencies": {
    "airbnb-browser-shims": "^2.1.0",
    "archiver": "^2.1.1",
    "autoprefixer": "^8.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.7.0",
    "babylonjs": "^3.2.0",
    "browserslist": "^3.2.8",
    "cannon": "^0.6.2",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "cssnano-preset-advanced": "^4.0.0-rc.2",
    "earcut": "^2.1.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "html-loader": "^0.5.5",
    "html-minifier-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.2.0",
    "offline-plugin": "^5.0.3",
    "oimo": "^1.0.9",
    "postcss": "^6.0.22",
    "postcss-loader": "^2.1.5",
    "postcss-scss": "^1.0.5",
    "postcss-uncss": "^0.16.1",
    "precss": "^3.1.2",
    "preload-webpack-plugin": "^2.3.0",
    "prettier": "^1.13.2",
    "request": "^2.87.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "uncss": "^0.16.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.0",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "main": "index.html",
  "keywords": [
    "BabylonJS",
    "PWA",
    "JavaScript",
    "Mobile",
    "Game"
  ]
}
